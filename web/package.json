{
  "name": "new-eden-social-web",
  "version": "0.1.0",
  "private": true,
  "license": "GPL-3.0",
  "engines": {
    "node": "^10.16.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "exit 0 || ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "exit 0 || cd web && ngc -p ./angular-metadata.tsconfig.json",
    "localization": "ng xi18n --output-path locale",
    "prebuild": "eval \"echo \"$(cat src/environments/environment.ts.template)\"\" > src/environments/environment.ts",
    "build:client-and-server-bundles": "ng build --prod && ng run new-eden-social-web:server:production",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress",
    "serve:ssr": "node dist/server"
  }
}
